<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnSingle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="单链表" />

    <Button
        android:id="@+id/btnSingleReversal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="单链表反转" />

    <Button
        android:id="@+id/btnDouble"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="双链表" />

    <Button
        android:id="@+id/btnRecycle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="循环列表" />

    <Button
        android:id="@+id/btnSame"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="单链表回文字符串" />

    <Button
        android:id="@+id/btnLinkedCheck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="链表中环的检测" />

    <Button
        android:id="@+id/btnLinkedMerge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="两个有序列表的合并" />

    <Button
        android:id="@+id/btnLinkedDeleteN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="删除链表倒数第N个结点" />

    <Button
        android:id="@+id/btnLinkedMiddleNode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="求链表的中间节点" />
</LinearLayout>