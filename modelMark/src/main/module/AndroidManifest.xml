<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wynne.knowledge.mark">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".MarkApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".home.BookActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".memo.MemoActivity" />
        <activity android:name=".activity.DrawableActivity" />
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".art.ArtActivity" />
        <activity
            android:name=".art.charpter1.ChapterOneActivity"
            android:configChanges="orientation"
            android:launchMode="singleTask"
            android:process="com.wynne.knowledge.remote"
            android:taskAffinity=".home.xxx" />


        <activity android:name=".activity.CustomActivity" />
        <activity android:name=".activity.ShadowActivity" />
        <activity android:name=".jetpack.JetPackActivity" />
        <activity android:name=".activity.XFermodeActivity" />
        <activity android:name=".activity.AnimationPathActivity" />
        <activity android:name=".jetpack.PagingActivity" />
        <activity android:name=".interview.InterViewActivity" />
        <activity android:name=".interview.http.HttpActivity" />
        <activity android:name=".interview.optimize.OptimizeActivity" />
        <activity android:name=".interview.rxjava.RxJavaActivity" />
        <activity android:name=".interview.okhttp.OkHttpActivity" />
        <activity android:name=".interview.ThreadFactoryActivity" />
        <activity android:name=".interview.basejava.BaseJavaActivity" />
        <activity android:name=".art.charpter1.SecondActivity" />
        <activity android:name=".art.charpter3.ChapterViewActivity" />
        <activity android:name=".art.charpter8.WindowActivity" />
        <activity android:name=".art.charpter2.ChapterTwoActivity" />

        <service
            android:name=".art.charpter2.AIDLService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.wynne.aidl" />
                <category android:name="android.intent.category.DEFAULT" />

            </intent-filter>
        </service>
    </application>
</manifest>
