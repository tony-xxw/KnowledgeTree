<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wynne.knowledge.guide">

    <application
        android:name=".GuideApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".home.GuideActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".service.ServiceActivity"
            android:launchMode="singleTop" />

        <service android:name=".service.StandardService" />
        <service android:name=".service.BindService" />

        <activity
            android:name=".constrain.ConstraintActivity"
            android:launchMode="singleTop" />
        <activity android:name=".material.RecycleActivity" />
    </application>
</manifest>
