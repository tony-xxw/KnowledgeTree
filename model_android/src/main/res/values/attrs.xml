<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="flow_view">
        <attr name="selectType" format="enum">
            <enum name="NONE" value="1" />
            <enum name="SINGLE" value="2" />
            <enum name="SINGLE_IRREVOCABLY" value="3" />
            <enum name="MULTI" value="4" />
        </attr>
        <attr name="maxSelect" format="integer" />
        <attr name="flowTextColor" format="reference" />
        <attr name="flowTextSize" format="dimension" />
        <attr name="lineMargin" format="dimension" />
        <attr name="wordMargin" format="dimension" />
        <attr name="flowBackground" format="reference" />
        <attr name="flowTextPaddingLeft" format="dimension" />
        <attr name="flowTextPaddingTop" format="dimension" />
        <attr name="flowTextPaddingRight" format="dimension" />
        <attr name="flowTextPaddingBottom" format="dimension" />
    </declare-styleable>
</resources>